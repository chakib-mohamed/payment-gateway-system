  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run start:dev",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    },

    {
      "command": "npm run test:st",
      "name": "Run st tests",
      "request": "launch",
      "type": "node-terminal"
    },

    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}"
    }
  ]
}
